    AddTask\n    },\n    methods : {\n        changePage(page){\n            this.currentPage = page\n            this.fetchTask()\n        },\n\n        fetchTask(){\n            axios({\n                url: `http://localhost:3000/tasks`,\n                method: `get`,\n                headers: {\n                    access_token : localStorage.getItem('access_token')\n                }\n            })\n            .then(response => {\n                this.tasks = response.data\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        },\n\n        editData(data){\n            this.editTask = data\n            this.currentPage = 'edit'\n        },\n    },\n    created(){\n        if(localStorage.getItem('access_token')){\n            this.changePage('home')\n            this.fetchTask()\n        }else {\n            this.changePage('login')\n        }\n    }\n}\n</script>\n\n<style scope>\n\n\n    #board{\n    display: flex;\n    flex-flow: row wrap;\n    justify-content: space-evenly;\n    margin-top: 20px;\n    }\n\n    #addBtn{\n        margin-left: auto;\n    }\n\n    #home_title{\n      text-align: center;\n      margin-top: 30px;\n      font-family: 'Poppins', sans-serif;\n      color: white;\n    }\n\n    \n</style>"},"lineCount":null}},"error":null,"hash":"3f96745e4a7d2c93bbe366620de6a444","cacheData":{"env":{}}}