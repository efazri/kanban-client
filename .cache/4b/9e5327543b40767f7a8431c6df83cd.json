ost:3000/tasks`,\n                method: `get`,\n                headers: {\n                    access_token : localStorage.getItem('access_token')\n                }\n            })\n            .then(response => {\n                this.tasks = response.data\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        }\n    },\n    created(){\n        if(localStorage.getItem('access_token')){\n            this.changePage('home')\n            this.fetchTask()\n        }\n        else {\n            this.changePage('login')\n        }\n    }\n}\n</script>\n\n<style scope>\n    #board{\n    display: flex;\n    flex-flow: row wrap;\n    justify-content: space-evenly;\n    margin-top: 20px;\n    \n\n  }\n\n  #home_title{\n      text-align: center;\n      margin-top: 30px;\n      font-family: 'Poppins', sans-serif;\n  }\n</style>"},"lineCount":null}},"error":null,"hash":"131df8312315439c2597d8f4c928a5e5","cacheData":{"env":{}}}